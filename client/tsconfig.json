{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "types": [
      "node",
      "core-js"
    ],
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  }
}
